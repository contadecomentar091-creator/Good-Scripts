	local canal;
	while true do
		if (FlatIdent_6B92D == 1) then
			if canal then
				local FlatIdent_95359 = 0;
				while true do
					if (0 == FlatIdent_95359) then
						canal.OnIncomingMessage = function(mensagem)
							ProcessarComando(mensagem.Text);
						end;
						print("‚úÖ Sistema de comandos ativo!");
						break;
					end
				end
			end
			break;
		end
		if (FlatIdent_6B92D == 0) then
			wait(3);
			canal = TextChatService.TextChannels:FindFirstChild("RBXGeneral");
			FlatIdent_6B92D = 1;
		end
	end
end);
