local UntagAllButton = TagsSection:Button({Title="Remover Todas Tags",Icon="eye-off",Callback=function()
	local FlatIdent_622B0 = 0;
	while true do
		if (FlatIdent_622B0 == 0) then
			SendCommand(";untag all");
			WindUI:Notify("Tags", "üö´ Todas as tags foram removidas!");
			FlatIdent_622B0 = 1;
		end
		if (FlatIdent_622B0 == 1) then
			for _, player in ipairs(Players:GetPlayers()) do
				if (player.Character and player.Character:FindFirstChild("Head")) then
					local FlatIdent_98327 = 0;
					local head;
					local tag;
					while true do
						if (FlatIdent_98327 == 0) then
							head = player.Character.Head;
							tag = head:FindFirstChild("SpecialTag");
							FlatIdent_98327 = 1;
						end
						if (FlatIdent_98327 == 1) then
							if tag then
								tag:Destroy();
							end
							break;
						end
					end
				end
			end
			break;
		end
	end
end});
